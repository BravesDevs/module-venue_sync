{
  "enctype": "multipart/form-data",
  "name": "default",
  "template": "form",
  "method": "POST",
  "model_class": "\\module\\venuesync\\model\\config_vendor_types",
  "children": [
    {
      "helper": "\\kernel\\form",
      "method": "panel",
      "class": "align-center",
      "name": ["Action Bar"],
      "template": "panel",
      "children": [
        {
          "helper": "\\kernel\\form",
          "method": "cancel",
          "name": ["action", "cancel"],
          "template": "cancel"
        },
        {
          "helper": "\\kernel\\form",
          "method": "menu",
          "template": "menu"
        },
        {
          "helper": "\\kernel\\form",
          "method": "submit",
          "label": "Submit",
          "name": ["action", "submit"],
          "template": "submit"
        }
      ]
    },
    {
      "helper": "\\kernel\\form",
      "method": "block",
      "label": "Basic Information",
      "compute_field_sequence": "1",
      "empty": "1",
      "show_collapsed": "0",
      "not_empty": "0",
      "open_on_focus": "0",
      "template": "block",
      "children": [
        {
          "colspan": "3",
          "name": ["config_vendor_types", "vendor_type"],
          "label": "vendor_type",
          "method": "input",
          "helper": "\\kernel\\form",
          "template": "input"
        },
        {
          "helper": "\\kernel\\form",
          "method": "input",
          "label": "seq",
          "colspan": "3",
          "empty": "1",
          "name": ["config_vendor_types", "seq"],
          "template": "input"
        }
      ]
    },
    {
      "name": ["Tab"],
      "helper": "\\kernel\\form",
      "method": "tab",
      "template": "tab",
      "children": [
        {
          "helper": "\\kernel\\form",
          "method": "subform",
          "label": "Permissions",
          "sub_form_model_class": "\\module\\core\\model\\listviews",
          "sub_form_name": "SYSTEM_ACL_REVISIONS",
          "overwrite_sub_form_model": "1",
          "show_collapsed": "0",
          "not_empty": "0",
          "name": ["ACL & Revisions"],
          "template": "subform"
        },
        {
          "name": ["Logs"],
          "label": "Logs",
          "sub_form_model_class": "\\module\\core\\model\\listviews",
          "sub_form_name": "SYSTEM_LOGS",
          "description": "Visible only to Admin role",
          "helper": "\\kernel\\form",
          "method": "subform",
          "overwrite_sub_form_model": "0",
          "show_collapsed": "0",
          "template": "subform"
        }
      ]
    }
  ]
}